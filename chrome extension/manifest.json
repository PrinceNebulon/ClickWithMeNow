  "web_accessible_resources": [
    "cwmn.png",
    "cwmn2.png"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["*://apps.inindca.com/*"],
      "css": ["inindca.css", "cwmnagentmenu.css"],
      "js": ["jquery-2.1.1.js", "clickwithmenow.js", "inindca.js"]
    }
  ]
}
