{
  "manifest_version": 2,
  "name": "ClickWithMeNow for PureCloud",
  "version": "2.1",
  "minimum_chrome_version": "23",
  "web_accessible_resources": [
    "img/cwmn.png",
    "img/cwmn2.png"
  ],
  "content_scripts": [
    {
      "matches": ["*://apps.inindca.com/directory/*"],
      "run_at": "document_end",
      "js": ["common/jquery-2.1.1.js", "cwmn/directory_link_injector.js"]
    },
    {
      "matches": ["*://apps.inindca.com/contact-center/*"],
      "run_at": "document_end",
      "js": ["common/jquery-2.1.1.js", "cwmn/contact-center_link_injector.js"]
    },
    {
      "matches": ["*://inindca-webchat-test.s3-website-us-east-1.amazonaws.com/*"],
      "run_at": "document_end",
      "js": ["common/jquery-2.1.1.js", "cwmn/customer_link_injector.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img/cwmn_small.png",
    "default_popup": "common/cwmnlinks.html"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}