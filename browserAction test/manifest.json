{
   "manifest_version": 2,
   "description": "MEH!",
   "name": "My First Extension",
   "permissions": [
      "tabs", "http://*/*", "https://*/*", "file://*/*"
   ],
   "version": "0.1",
   "content_scripts": [{
      "run_at": "document_end",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-2.1.1.js","jquery.color-2.1.2.js"]
   }],
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   }
}